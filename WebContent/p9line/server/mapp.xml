<?xml version="1.0" encoding="UTF-8" ?>
<maxml version="2.0" xmlns="http://www.nj.fiberhome.com.cn/map"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.nj.fiberhome.com.cn/map maxml-2.0.xsd">
  <config>
    <htmlformat wellformat="true" />
    <filepreviewconfig wordtohtml="true"/>
    <domain address="www.p9line.cn" name="p9line"/>
  </config>
  <services>
  	<http-service>
  		<forward pattern="/index" path="index.jsp"/>
  		<forward pattern="/goods" path="goods.jsp"/>
  		<forward pattern="/search" path="search.jsp"/>
  		<forward pattern="/category" path="category.jsp"/>
  		<forward pattern="/topic" path="topic.jsp"/>
  		<forward pattern="/user" path="user.jsp"/>
  		<forward pattern="/login" path="login.jsp"/>
  		<forward pattern="/captcha" path="captcha.jsp"/>
  		<forward pattern="/logout" path="logout.jsp"/>
  		<forward pattern="/add2cart" path="add2cart.jsp"/>
  		<forward pattern="/carts" path="carts.jsp"/>
  		<forward pattern="/cartsUpdate" path="cartsUpdate.jsp"/>
  		<forward pattern="/checkout" path="checkout.jsp"/>
  		<forward pattern="/getAddress" path="getAddress.jsp"/>
  		<forward pattern="/getRegion" path="getRegion.jsp"/>
  		<forward pattern="/saveAddress" path="saveAddress.jsp"/>
  		<forward pattern="/delAddress" path="delAddress.jsp"/>
  		<forward pattern="/cartsPayment" path="cartsPayment.jsp"/>
  		<forward pattern="/cartsDone" path="cartsDone.jsp"/>
  		<forward pattern="/getOrder" path="getOrder.jsp"/>
  		<forward pattern="/add2favorite" path="add2favorite.jsp"/>
  		<forward pattern="/getFavorite" path="getFavorite.jsp"/>
  		<forward pattern="/delFavorite" path="delFavorite.jsp"/>
  		<forward pattern="/addressList" path="addressList.jsp"/>
  		<forward pattern="/saveMyAddress" path="saveMyAddress.jsp"/>
  		<forward pattern="/delMyAddress" path="delMyAddress.jsp"/>
  	</http-service>
  </services>
</maxml>